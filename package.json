{
  "name": "next-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '{**/*,*}.{js,ts,jsx,tsx}' --cache",
